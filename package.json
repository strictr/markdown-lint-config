{
    "name": "@strictr/markdown-lint-config",
    "version": "1.3.1",
    "description": "A strict markdown lint configuration",
    "main": "index.json",
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "scripts": {
        "release": "standard-version"
    },
    "peerDependencies": {
        "markdownlint": "0.18.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/strictr/markdown-lint-config.git"
    },
    "keywords": [
        "lint",
        "markdownlint",
        "markdown",
        "configuration"
    ],
    "author": "Teodor Kurtev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/strictr/markdown-lint-config/issues"
    },
    "homepage": "https://github.com/strictr/markdown-lint-config#readme",
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "cz-conventional-changelog": "^3.0.2",
        "husky": "^4.0.10",
        "standard-version": "^7.0.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
